package com.psl.assignment5;

public class xo {

    
	
	
	public static void main(String[] args) {
	char mat[][]={{'x','x','o'},{'x','x','o'},{'x','o','o'}};
int j,f,g;
	for(int i=0;i<3;i++){
		f=0;g=0;
	   for( j=0,f=0,g=0;j<3;j++)
	   { 
		   if(mat[i][j]=='x')
			   f++;
		   else if(mat[i][j]=='o')
			   g++;
		   
	   
		   if((mat[i][j]=='x'&& i==j))
			   f++;
	   }
	
	
		 
} 
}
