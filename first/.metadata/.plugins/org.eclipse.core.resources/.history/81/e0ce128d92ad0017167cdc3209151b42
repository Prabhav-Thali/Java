package com.psl.assignment5;

public class xo {

    
	
	
	public static void main(String[] args) {
	char mat[][]={{'x','x','o'},{'x','x','o'},{'x','o','o'}};
int j,x,o,f=0;
	for(int i=0;i<3;i++)
	   for( j=0,x=0,o=0;j<3;j++)
	   {
		   if((mat[i][j]=='x'&& i==j)||(mat[i][j]=='o'&& i==j))
			   x++;
		 
} 
}
